# Database
DATABASE_URL=postgresql://analytics_readonly:readonly_pass@postgres:5432/analytics_db
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10

# Redis
REDIS_URL=redis://redis:6379/0
REDIS_TTL=300

# Groq API
GROQ_API_KEY=gsk_your_groq_api_key_here
GROQ_MODEL=llama-3.3-70b-versatile

# OpenTelemetry
OTEL_ENABLED=true
OTEL_SERVICE_NAME=analytics-agent
OTEL_EXPORTER_OTLP_ENDPOINT=http://jaeger:4318

# SQL Safety
SQL_QUERY_TIMEOUT=30
SQL_MAX_ROWS=10000
SQL_MAX_JOINS=3

# API
API_PORT=8000
DEBUG=true